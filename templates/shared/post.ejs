<div class="card">
    <h5 class="card-header"><%= post.title %></h5>
    <div class="list-group list-group-flush">
        <p class="list-group-item m-0"><%= post.content.split("\n\n")[0].substring(0,150) %>...</p>
        <a class="btn btn-secondary list-group-item" href="/article/<%= post.id %>" >Lire la suite</a>
    </div>
</div>